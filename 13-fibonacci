public Integer fibonacci(Integer n) {
    //code here
    // iteger s=1;
     if (n <= 0) {
        return 0;
    } else if (n == 1) {
        return 1;
    }
    integer a=0;
    integer b=1;
    for(integer i=2;i<=n;i++){
        integer temp=a+b;
        a=b;
        b=temp;

    }
    return b;
}