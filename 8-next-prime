public Integer nextPrime(Integer num) {
    //code here
    // num=+1;
    while(true){
        num+=1;
        Boolean isPrime = true;
        for(integer i=2;i <= num/2;i++){
            if(math.mod(num,i)==0){
                isPrime=false;
                break;
            }
        }
        if(isPrime){
        return num;
        }
    }
    return num;
}