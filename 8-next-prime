public Integer nextPrime(Integer num) {
    //code here
    // num=+1;
    num+=1;
    while(true){
        Boolean isPrime = true;
        for(integer i=2;i<num/2;i++){
            if(math.mod(num,i)==0){
                break;
            }
        }
        if(isPrime){
        return num;
        }
    num+=1;
    }
    return num;
}